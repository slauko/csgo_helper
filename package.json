{
	"author": "slauko",
	"name": "csgo-helper",
	"description": "small helper tool for csgo",
	"version": "0.2.5",
	"private": true,
	"main": "public/electron.js",
	"build": {
		"productName": "csgo_helper",
		"executableName": "CSGO_Helper",
		"files": [
			"build/**/*",
			"public/*"
		],
		"win": {
			"icon": "public/icon.ico"
		},
		"nsis": {
			"artifactName": "CSGO_Helper Setup.${ext}",
			"installerIcon": "public/icon.ico"
		},
		"linux": {},
		"mac": {}
	},
	"dependencies": {
		"@nut-tree/nut-js": "^2.0.1",
		"bootstrap": "^5.1.3",
		"electron-fetch": "^1.7.4",
		"electron-overlay-window": "^2.0.1",
		"electron-serve": "^1.1.0",
		"electron-updater": "^5.0.1",
		"ffi-napi": "^4.0.3",
		"keycode": "^2.2.1",
		"memoryjs": "https://github.com/slauko/memoryjs",
		"node-fetch": "^3.2.4",
		"react-bootstrap": "^2.3.1",
		"react-draggable": "^4.4.5",
		"react-fps-stats": "^0.2.3",
		"sass": "^1.51.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"electron": "wait-on http://localhost:3000 && electron .",
		"electron-dev": "concurrently \"yarn electron\" \"yarn start\"",
		"preelectron-pack": "yarn build",
		"electron-pack": "electron-builder"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^13.0.0",
		"@testing-library/user-event": "^13.2.1",
		"concurrently": "^7.1.0",
		"electron": "^18.2.0",
		"electron-builder": "^23.0.3",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"react-scripts": "5.0.1",
		"wait-on": "^6.0.1",
		"web-vitals": "^2.1.0"
	}
}
